## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    module = bld.create_ns3_module('C2C-IP', ['core'])
    module.source = [
        'C2C-IP-app.cc',
        ]
    headers = bld(features='ns3header')
    headers.module = 'C2C-IP'
    headers.source = [
        'C2C-IP-app.h',
        ]

